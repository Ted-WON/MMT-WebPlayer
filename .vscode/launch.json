{
    "version": "0.2.0",
    "configurations": [

        {
            "type": "node",
            "request": "launch",
            "protocol": "inspector",
            "name": "Launch mpu-tester",
            "program": "${workspaceRoot}/mpu-manager/mpu-tester.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceRoot}/Client/samples/app.js",
            "cwd": "${workspaceRoot}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Server&Client Test",
            "program": "${workspaceRoot}/Client/mmt-client.js",
            "cwd": "${workspaceRoot}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Server Debug",
            "program": "${workspaceRoot}/Server/mmt-server.js",
            "cwd": "${workspaceRoot}"
        },
        {
            "name": "Launch Chrome against localhost",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:8080/Users/daehee/Git/MMT-WebPlayer/Client/samples/index.html",
            "webRoot": "${workspaceRoot}"
        },
        {
            "name": "Attach to Chrome",
            "type": "chrome",
            "request": "attach",
            "port": 9222,
            "webRoot": "${workspaceRoot}"
        }
    ]
}